<UserControl x:Class="RegionNavigator1.Views.NavigatorButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
        <StackPanel>
            <Label Content="Choose Action:" />
            <Button Command="{Binding ChooseACommand}" Content="Choose A" />
            <Button Command="{Binding ChooseBCommand}" Content="Choose B" />
        </StackPanel>
    </Grid>
</UserControl>
